          </div>
        </div>
        <div class="flex items-center justify-start w-full p-2 font-sans font-normal md:px-14">
          <p class="font-sans text-xl md:text-4xl">
            <%= storeInfo.store_description %>
          </p>
        </div>

        <div class="flex w-full md:px-14">
          <div class="flex flex-col items-start justify-between w-full gap-8 p-8 flex-nowrap lg:w-4/5">
            <!--store details card-->
            <% storeCoupons.forEach(element=> { %>
              <div class="flex w-full flex-nowrap drop-shadow-lg h-52">
                <div class="relative w-2/6">
                  <img src="<%= element.coupon_image %>" alt="" class="object-cover w-full h-full rounded-l-lg">
                  <span
                    class="absolute bottom-0 right-0 z-10 flex flex-wrap items-center justify-center px-0 py-2 text-lg font-bold text-white rounded-tl-xl bg-k-red">
                    <%= element.coupon_percentage %> <span class="w-full text-sm text-center">OFF</span>
                  </span>
                </div>
                <div class="flex flex-row flex-wrap items-center justify-center w-4/6 text-black bg-white rounded-r-lg">
                  <div class="flex flex-col w-full px-8">
                    <h2 class="font-sans text-xl font-bold text-left sm:text-3xl drop-shadow-md">
                      <%= storeInfo.store_title %>
                    </h2>
                    <p class="mt-4 font-sans text-sm font-light">
                      <%= element.coupon_description %>
                    </p>
                  </div>
                  <div class="flex items-center w-full px-8">
                    <img src="<%= element.coupon_image %>"
                      class="left-0 flex items-center justify-center rounded-full brand-logo">
                    <button class="ml-auto text-sm font-light text-white rounded-lg w-36 h-9 bg-k-red">Copy &
                      Shop</button>
                  </div>
                </div>
              </div>
              <% }); %>
          </div>
          <!--Useful Links-->
          <div class="flex flex-col pt-8">
            <div class="flex justify-end p-2 bg-white rounded-lg shadow-xl min-h-[30%] md:mt-0 links w-full">
              <div class="flex w-full px-2 py-4 bg-white">
                <h4 class="w-full font-sans text-base font-bold text-left">Useful Links</h4>
              </div>
            </div>

            <% storeFeaturers.forEach(element => { %>
            <div class="flex flex-col items-stretch justify-center w-full gap-0 mt-2 bg-white shadow rounded-xl">
              <div class="flex items-center justify-between gap-5 px-6 py-6 border-l-8 shadow-md border-k-red rounded-xl">
                <img loading="lazy"
                  src="<%= element.image %>"
                  class="aspect-square object-cover object-center w-[50px] justify-center items-center overflow-hidden shrink-0 max-w-full" />
                <div class="items-stretch self-stretch flex grow basis-[0%] flex-col">
                  <div class="text-xl font-bold leading-8 tracking-normal text-slate-800 whitespace-nowrap">
                    <%= element.title %>
                  </div>
                  <div class="text-neutral-500 text-sm leading-5 tracking-wide mt-1.5">
                    <%= element.description %>
                  </div>
                </div>
              </div>
            </div>
            <% }); %>
          </div>
        </div>

      </div>
    </div>
    <!--end main-->

    <%- include('footer') %>
      <%- include('scripts') %>
  </body>

</html>