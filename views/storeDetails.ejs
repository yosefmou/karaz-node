<!DOCTYPE html>
<html lang="en">

<%- include('head') %>

  <body id="app" class="block font-sans">
    <div class="background-images">
    </div>
    <div class="mb-28 main">
      <!--start hero-->
      <div class="w-full pt-32">
        <!--header component-->
        <%- include('navigation') %>
          <!--end header component-->
          <!--banner component-->
          <div class="relative flex w-full">
            <img src="/images/pexels-antonio-sokic-3839432 1.svg" class="w-full short-banner-img ">
            <div class="absolute overlay-short-banner"></div>
            <p class="absolute -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2 bg-clip-text">
              <span class="font-sans text-5xl font-bold text-center text-white uppercase md:text-9xl">
                <%= storeInfo.store_title %>
              </span>
            </p>
          </div>
          <!--end banner component-->
      </div>
      <!--end hero-->

      <!--start main-->
      <div class="container mx-auto my-0">
        <!--store details logo background-->
        <div class="absolute -z-10">
          <div class="text-[10rem] break-words opacity-[0.03]">
            <%= storeInfo.store_title %>
          </div>
        </div>
        <!--end store details logo background-->
        <!--start mais store details-->
        <div class="flex flex-wrap items-center justify-between w-full px-2 py-8 font-sans font-normal md:px-14">
          <div class="flex items-center justify-start ml-2 space-x-2">
            <h3 class="font-sans text-3xl font-normal">
              <%= storeInfo.store_title %>
            </h3>
